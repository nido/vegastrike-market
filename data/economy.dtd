<!ELEMENT vector ((CargoType|Cargo|ProductionOption|Factory)+)>
<!ATTLIST vector
	type CDATA #REQUIRED
>

<!ELEMENT Factory (ProductionOption+)>
<!ATTLIST Factory>

<!ELEMENT ProductionOption (Cargo, Cargo)>
<!ATTLIST ProductionOption
	active (true) #IMPLIED
>

<!ELEMENT Cargo (CargoType+)>
<!ATTLIST Cargo
	type (in|out) #IMPLIED
>

<!ELEMENT CargoType (#PCDATA)>
<!ATTLIST CargoType
	name CDATA #REQUIRED
	category CDATA #REQUIRED
	mass CDATA #REQUIRED
	volume CDATA #REQUIRED
	price CDATA #REQUIRED
>
